{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "./src/**/*.astro"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "noImplicitAny": false,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
